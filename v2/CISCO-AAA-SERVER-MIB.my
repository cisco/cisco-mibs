CISCO-AAA-SERVER-MIB DEFINITIONS ::= BEGIN


IMPORTS
        MODULE-IDENTITY,
        NOTIFICATION-TYPE,
        OBJECT-TYPE,
        Counter32,
        IpAddress
                FROM SNMPv2-SMI
        MODULE-COMPLIANCE, 
        OBJECT-GROUP
                FROM SNMPv2-CONF
        TimeInterval, DisplayString, 
        TruthValue, RowStatus
                FROM SNMPv2-TC
        Unsigned32
                FROM CISCO-TC
        ciscoExperiment
                FROM CISCO-SMI;


ciscoAAAServerMIB MODULE-IDENTITY
        LAST-UPDATED    "202303010000Z"
        ORGANIZATION    "Cisco Systems, Inc."
        CONTACT-INFO
                "       Cisco Systems\n\
                        Customer Service\n\
\n\
                Postal: 170 W Tasman Drive\n\
                        San Jose, CA  95134\n\
                        USA\n\
\n\
                   Tel: +1 800 553-NETS\n\
\n\
                E-mail: cs-aaa@cisco.com"
        DESCRIPTION
                "The MIB module for monitoring communications and status\n\
                 of AAA Server operation\n\
                "
        REVISION         "202303010000Z"
        DESCRIPTION
                "Added objects to support AAA server state monitoring and\n\
                 counter for authentication retransmissions\n\
                             casWiredServerState\n\
                             casAuthenRetrans\n\
                "
