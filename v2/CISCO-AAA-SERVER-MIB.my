--
-- Server availability
--

casState OBJECT-TYPE
        SYNTAX      INTEGER {
                        up(1),
                        dead(2)
                    }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current state of the server at system\n\
\n\
                 up(1)   - Server is alive and responding to requests.\n\
\n\
                 dead(2) - Server is dead and fails to respond to requests\n\
\n\
             A server is marked dead if dead-criteria is met\n\
\n\
             A server is marked up again when \n\
             1. configured dead timer expires\n\ 
             2. when server responds to probes AAA client\n\
\n\
             The initial value of casState is 'up(1)' at system\n\
             re-initialization. This will only transistion to 'dead(2)'\n\
             if an attempt to communicate fails and dead-criteria is met\n\
            "
        ::= { casStatisticsEntry 25 }

casCurrentStateDuration OBJECT-TYPE
        SYNTAX      TimeInterval
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object provides the elapsed time the server has\n\
             been in its current state as shown in casState.\n\
            "
        ::= { casStatisticsEntry 26 }

casPreviousStateDuration OBJECT-TYPE
        SYNTAX      TimeInterval
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "This object provides the elapsed time the server was\n\
             been in its previous state prior to the most recent\n\
             transistion of casState.\n\
\n\
             This value is zero if the server has not changed state.\n\
            "
        ::= { casStatisticsEntry 27 }

casTotalDeadTime OBJECT-TYPE
        SYNTAX      TimeInterval
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The total elapsed time this server's casState has\n\
             had the value 'dead(2)' since system re-initialization.\n\
            "
        ::= { casStatisticsEntry 28 }

casDeadCount OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of times this server's casState has\n\
             transitioned to 'dead(2)' since system re-initialization.\n\
            "
        ::= { casStatisticsEntry 29 }

casWiredServerState OBJECT-TYPE
        SYNTAX      INTEGER {
                        up(1),
                        dead(2)
                    }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Current state of this server for wired sessions.\n\
\n\
                 up(1)   - Server is alive and responding to requests.\n\
\n\
                 dead(2) - Server is dead and fails to respond to requests\n\
\n\
             A server is marked dead if dead-criteria is met\n\
\n\
             A server is marked up again when \n\
             1. configured dead timer expires\n\ 
             2. when server responds to probes AAA client\n\
\n\
             The initial value of casWiredServerState is 'up(1)' at system \n\
             re-initialization. This will only transistion to 'dead(2)'\n\
             if an attempt to communicate fails and dead-criteria is met\n\
            "
        ::= { casStatisticsEntry 30 }

casAuthenRetrans OBJECT-TYPE
        SYNTAX      Counter32
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The number of authentication retransmissions sent to\n\
             this server since the first request times out.  \n\
\n\
             Retransmissions due to request timeouts are\n\
             counted as distinct requests.\n\
            "
        ::= { casStatisticsEntry 31 }




