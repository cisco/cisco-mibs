-- *****************************************************************
-- cseSecySessionTable - MACSEC Session State Monitoring Table
-- *****************************************************************

cseSecySessionTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF CseSecySessionEntry 
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "A table containing the operational status information for
        MACSEC sessions on each MAC security capable interface.
        This table provides OIDs to monitor the operational status
        of MACSEC sessions (up, down, negotiating)."
    ::= { ciscoSecyExtMIBStatsObjects 7 }

cseSecySessionEntry OBJECT-TYPE
    SYNTAX          CseSecySessionEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "An entry containing the operational status of a MACSEC
        session on a MAC security capable interface."
    INDEX           { secyIfInterfaceIndex } 
    ::= { cseSecySessionTable 1 }

CseSecySessionEntry ::= SEQUENCE {
        cseSecySessionOperStatus    INTEGER,
        cseSecySessionOperStatusStr DisplayString
}

cseSecySessionOperStatus OBJECT-TYPE
    SYNTAX          INTEGER {
                        up(1),
                        down(2),
                        negotiating(3)
                    }
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The operational status of the MACSEC session on this interface.

        up(1)          - MACSEC session is secured and operational.
                         MKA has successfully negotiated keys and both
                         TX and RX secure channels are active.

        down(2)        - MACSEC session is not operational.
                         Either MACSEC is not enabled or MKA session
                         has not been established.

        negotiating(3) - MACSEC session is in the process of being
                         established. MKA has been secured but secure
                         channels are not yet fully operational." 
    ::= { cseSecySessionEntry 1 }

cseSecySessionOperStatusStr OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (0..32))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "A textual representation of the operational status of the
        MACSEC session on this interface. Possible values are:
        'up', 'down', or 'negotiating'." 
    ::= { cseSecySessionEntry 2 }


