        group   bandwidthUsage
        description "(null)"
        characteristics "-K NONE"
        (
                readonly counter        bandwidthUsageCurrent
                description     "The bandwidth currently consumed.  The measurement unit is in
                 megabits per second (1,000,000 bits/second)."
                characteristics "-N bandwidthUsageCurrent -O 1.3.6.1.4.1.437.1.1.3.7.1 -T COUNTER -A RO -X equal -F 0"

                readonly int    bandwidthUsageMaxPeakEntries
                description     "The maximum number of entries bandwidthUsagePeakTable
                 can have."
                characteristics "-N bandwidthUsageMaxPeakEntries -O 1.3.6.1.4.1.437.1.1.3.7.2 -T INTEGER -A RO -X equal -F 0"

                readwrite enum bandwidthUsagePeakIntervalE      bandwidthUsagePeakInterval
                description     "This object specifies the length of time which forms a
                 peak bandwidth capture interval.  The default is
                 oneday(24).  A write to this object with any new value
                 restarts the peak bandwidth capture at the first interval.
                 In other word, the bandwidthUsagePeakTable will be cleared
                 and entry number 1 will record the peak for a new first
                 interval."
                characteristics "-N bandwidthUsagePeakInterval -O 1.3.6.1.4.1.437.1.1.3.7.3 -T INTEGER -A RW -X equal -F 0"

                readwrite enum bandwidthUsagePeakRestartE       bandwidthUsagePeakRestart
                description     "Set to restart(2) to clear the bandwidthUsagePeakTable
                 and restart the peak bandwidth capturing at the current
                 interval.  No action will be taken if this object is set
                 to noRestart(1).  This object always returned noRestart(1)
                 when read."
                characteristics "-N bandwidthUsagePeakRestart -O 1.3.6.1.4.1.437.1.1.3.7.4 -T INTEGER -A RW -X equal -F 0"

                readonly int    bandwidthUsageCurrentPeakEntry
                description     "A value identifying an instance of the bandwidthUsagePeakIndex
                 of the bandwidthUsagePeakTable where the peak bandwidth
                 measurement is most current."
                characteristics "-N bandwidthUsageCurrentPeakEntry -O 1.3.6.1.4.1.437.1.1.3.7.5 -T INTEGER -A RO -X equal -F 0"

        )

