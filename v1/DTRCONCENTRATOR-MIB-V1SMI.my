      enterprises, Counter, TimeTicks
               FROM RFC1155-SMI
      OBJECT-TYPE
              FROM RFC-1212
      DisplayString
                FROM RFC1213-MIB;
--      OBJECT-TYPE
--              FROM RFC-1212;

-- SNMPv1 Textual Conventions
